{
  "name": "onlycare-socket-server",
  "version": "1.0.0",
  "description": "Real-time WebSocket server for OnlyCare video calling",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start server.js --name onlycare-socket",
    "pm2:stop": "pm2 stop onlycare-socket",
    "pm2:restart": "pm2 restart onlycare-socket",
    "pm2:logs": "pm2 logs onlycare-socket",
    "pm2:monit": "pm2 monit"
  },
  "keywords": [
    "websocket",
    "socket.io",
    "realtime",
    "video-calling",
    "onlycare"
  ],
  "author": "OnlyCare Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}









